{
  "mcp": {
    "python_executor": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-python-executor",
        "--venv-path", "${VICTORIA_HOME}/venvs/adtech",
        "--allowed-packages", "pandas,numpy,matplotlib,seaborn,plotly,openpyxl,xlsxwriter,xlrd,scikit-learn,scipy,statsmodels,jupyter,ipython,requests,beautifulsoup4"
      ]
    },
    "excel_advanced": {
      "type": "stdio",
      "command": "uvx", 
      "args": [
        "mcp-server-excel-advanced",
        "--workspace-path", "${VICTORIA_HOME}/data"
      ]
    }
  }
}

